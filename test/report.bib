@article{cpbehavior,
  title={Coherence Potentials Encode Simple Human Sensorimotor Behavior},
  author={Parameshwaran, D and Crone, N and Thiagarajan, T},
  journal={PLoS one},
  volume={7},
  number={2},
  pages={},
  year={2012},
  publisher={},
  notes={This paper finds coherence potentials in ECoG data from humans. Their grid is 64 cm^2, so pretty big. It spans multiple brain regions. Their most interesting finding is that some CPs occur for the whole multi-minute behavior period and can return after several-second quiet periods, indicating they don't need to overlap in time. They found that some CPs were strongly associated with certain behaviors and were biased towards electrodes which encoded those behaviors: the right hand digit electrode which moved the right hand digits when stimulated was a strong host for the RH CP, and was often the first to generate the CP within a behavior window. They found that CPs that were associated with analogous tasks, like right vs left hand movements, had similar CPs, though this was with small sample size. Some CPs were very predictive of a future behavior, with R>0.95.},
  questions={}
}