}

@article{entropyinhibition,
  title={Robust entropy requires strong and balanced excitatory and inhibitory synapses},
  author={Agrawal, V and Cowley, AB and Alfaori, Q and Restrepo, J and Larremore, D and Shew, W},
  journal={arXiv:1804.05266},
  volume={},
  number={},
  pages={},
  year={2018},
  publisher={},
  notes={A paper which uses an automata network model with inhibitory and excitatory weights. The paper uses a mean-field-esque technique to simplify the network into a random walk, then calculates the master random walk equation to find the steady-state behavior of excitation (fraction of nodes excited). They use this steady-state distribution to find the effect of connection strength and exc/inh fraction on entropy and entropy parameter-stability. They find that entropy is maximized between the down and up states, and whats more, find that robust entropy is found for very low inhibition-to-excitation ratios. They claim that high entropy is good for maximizing the repertoire of available states, but could hurt signal-to-noise.},
  questions={}
}

