@article{biologicalsoc,
  title={Synaptic Plasticity Enables Adaptive Self-Tuning Critical Networks},
  author={Stepp, N and Plenz, D and Srinivasa, N},
  journal={PLOS Computational Biology},
  volume={},
  number={},
  pages={},
  year={2015},
  publisher={PLOS},
  notes={This paper uses a complex biological LIF model with leak, synaptic conductances for GABA and Glutamatergic channels, and both STP and EI-STDP. It achieves both criticality and balance, where E and I currents generally balance each other. It achieves pretty intense stability: injecting a single spike only adds ~20 resultant spikes; more dramatic stimulation "perturbes" the system lightly, but it returns to its stable state. The network changes its excitatory values on purturbation to values different than they were initially at, indicating that the system may have an infinite number of fixed points it can relax at. The network also achieves what they call "persistent asynchronous background activity," which basically means high-Fanno-Factor firing without input. This is a really cool paper which would be a good one to emulate, since their network seems like it has potential for some really cool dynamics.},
  questions={}
}

@article{heterogeneousnetwork,
